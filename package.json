    "webpack-dev-server": "2.5.0",
    "webpack-merge": "4.1.0",
    "babel-core": "6.24.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-loader": "7.1.1",
    "flux": "3.1.2",
    "history": "^4.5.0",
    "css-loader": "0.28.4",
    "file-loader": "0.11.2",
    "font-loader": "0.1.2",
    "html-loader": "0.4.5",
    "json-loader": "0.5.4",
    "style-loader": "0.18.2",
    "sass-loader": "6.0.6",
    "svg-loader": "0.0.2",
    "url-loader": "0.5.9",
    "resolve-url-loader": "2.1.0",
    "keymirror": "0.1.1",
