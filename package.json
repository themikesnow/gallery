{
  "name": "gallery",
  "version": "1.0.0",
  "description": "Sample App",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://miguelnievesacevedo@bitbucket.org/miguelnievesacevedo/blackmamba-ui.git"
  },
  "keywords": [
    "Blackmamba"
  ],
  "author": "Miguel A. Nieves Acevedo",
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --p",
    "test": "jest",
    "lintOLD": "eslint src/**",
    "lint": "eslint --ext .jsx --ext .js src/**"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2"
  },
  "peerDependencies": {},
